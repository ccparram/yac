{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "extends": ["airbnb"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-console": "off",
      "no-use-before-define": "off",
      "react/jsx-filename-extension": "off",
      "react/prop-types": "off"
    }
}